config_version=5

[application]

config/name="Space Invaders Clone"
run/main_scene="res://Scenes/TitleScreen.tscn"
config/features=PackedStringArray("4.2", "C#", "GL Compatibility")
config/icon="res://icon.svg"

[display]

window/size/viewport_width=800
window/size/viewport_height=600
window/size/resizable=false

[dotnet]

project/assembly_name="Space Invaders Clone"

[editor]

naming/default_signal_callback_name="On{NodeName}{SignalName}"
naming/default_signal_callback_to_self_name="On{SignalName}"
naming/scene_name_casing=1

[file_customization]

folder_colors={
"res://Assets/": "blue",
"res://Scenes/": "yellow",
"res://Scripts/": "purple"
}

